alph <- 0.5
gam <- 0.5
delt <- 0.02
bet <- 0.05
aa <- 0.5
T <- 50


x[5] := 1.2

# Initial values
x[~] <- 1
k[~] <- ((delt+bet)/(1.0*aa*alph))^(1/(alph-1))
c[~] <- aa*k[~]^alph-delt*k[~]


# Model block
0 = c[t] + k[t] - aa*x[t]*k[t-1]^alph - (1-delt)*k[t-1]
0 = c[t]^(-gam) - (1+bet)^(-1)*(aa*alph*x[t+1]*k[t]^(alph-1) + 1 - delt)*c[t+1]^(-gam)